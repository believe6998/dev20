<%- include('./layouts/head',
{title: 'Home admin page'}) %>


<!-- Page Heading -->
<h1 class="h3 mb-2 text-gray-800 pt-4">Booking</h1>
<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header ">
        <strong><a href="/Admin/User/Create">Thêm bản ghi</a></strong>
    </div>
    <div class="card-body">
        <div class="table-responsive">

            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                <tr>
                    <th>TimeId</th>
                    <th>Time slot</th>
                </tr>
                </thead>
                <tbody>
                <% for (var i = 0; i < listTime.length; i++) { %>
                    <tr>
                        <td>   <%= listTime[i].id %></td>
                        <td>   <%= listTime[i].time %></td>
                    </tr>
                <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>


<%- include('./layouts/footer',
) %>
