<%- include('./layouts/head',{
title: 'Home Page'
}) %>
<%- include('./layouts/header') %>
<!-- Banner Area Starts -->
<section class="banner-area other-page">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1>Record</h1>
                <a href="index.html">Home</a> <span>|</span> <a href="contact.html">Contact Us</a>
            </div>
        </div>
    </div>
</section>
<!-- Banner Area End -->
<div class="container">
    <div class="section-top-border">
        <h3 class="mb-30 title_color">Record</h3>
        <table class="table table-hover">
            <thead class="table-head">
            <tr>
                <th scope="col">number </th>
                <th scope="col">Disease</th>
                <th scope="col">drug</th>
                <th scope="col">med ex date </th>
            </tr>
            </thead>
            <tbody>
            <% for (var i = 0; i <listRecord.length; i++) { %>
                <tr>
                    <th scope="row"><%= i + 1 %></th>
                    <td><%= listRecord[i].typeOfDisease %></td>
                    <td> <%= listRecord[i].drug %></td>
                    <td><%= listRecord[i].createAt %></td>
                </tr>
            <% } %>
            </tbody>
        </table>
    </div>
</div>

<%- include('./layouts/footer',
) %>